{
    "name": "bug-tracker-api",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "start": "npm run serve",
        "serve": "node dist/src/index.js",
        "watch-node": "nodemon dist/src/index.js",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "migrate:make": "knex --cwd src/db migrate:make -x ts",
        "migrate:latest": "knex --cwd src/db migrate:latest",
        "seed:make": "knex --cwd src/db seed:make -x ts",
        "seed:run": "knex --cwd src/db seed:run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tearingItUp786/bug-tracker-api.git"
    },
    "keywords": [],
    "author": "Taran 'tearing it up' Bains",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tearingItUp786/bug-tracker-api/issues"
    },
    "homepage": "https://github.com/tearingItUp786/bug-tracker-api#readme",
    "devDependencies": {
        "@types/bcrypt": "3.0.0",
        "@types/body-parser": "1.19.0",
        "@types/cors": "2.8.6",
        "@types/express": "4.17.6",
        "@types/jsonwebtoken": "8.3.9",
        "@types/lodash": "4.14.150",
        "@types/morgan": "^1.9.0",
        "@types/node": "13.11.1",
        "@types/pg": "7.14.3",
        "@typescript-eslint/eslint-plugin": "2.31.0",
        "@typescript-eslint/parser": "2.31.0",
        "body-parser": "1.19.0",
        "concurrently": "5.2.0",
        "cors": "2.8.5",
        "eslint": "6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "nodemon": "2.0.3",
        "prettier": "^2.0.5",
        "ts-node": "8.10.1",
        "typescript": "3.8.3"
    },
    "dependencies": {
        "@types/dotenv": "^8.2.0",
        "@types/http-status-codes": "^1.2.0",
        "bcrypt": "^4.0.1",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-validator": "^6.4.1",
        "http-status-codes": "^1.4.0",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.21.0",
        "lodash": "^4.17.15",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "pg": "^8.0.2"
    },
    "_moduleAliases": {
        "@utils": "dist/utils/",
        "@resources": "dist/resources/",
        "@config": "dist/config/index.js",
        "@knex": "dist/db/index.js"
    }
}
